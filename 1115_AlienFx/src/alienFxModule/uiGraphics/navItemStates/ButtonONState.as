package alienFxModule.uiGraphics.navItemStates {import com.dell.alienFx.GlobalAnim;import com.greensock.TweenLite;import com.greensock.plugins.TintPlugin;import com.greensock.plugins.TweenPlugin;import flash.display.MovieClip;import alienFxModule.uiGraphics.CreateHideControlButton;import alienFxModule.uiGraphics.navItemStates.abstract.Context_HideControl_States;import alienFxModule.uiGraphics.navItemStates.abstract.IState_ButtonONOFF;//Stop State;public class ButtonONState implements IState_ButtonONOFF {    private var buttons:Context_HideControl_States;    private var target:CreateHideControlButton;    private var numbers:MovieClip;    private var boxes:MovieClip;    public function ButtonONState(_buttons:Context_HideControl_States, _target:CreateHideControlButton) {        TweenPlugin.activate([TintPlugin]);        this.buttons = _buttons;        this.target = _target;        numbers = _target.target.numbers;        boxes = _target.target.boxes;    }    public function ON():void {    }    public function OFF():void {        TweenLite.to(target.hideLabel, GlobalAnim.TRANSISITION, {alpha: 0});        TweenLite.to(target.showLabel, GlobalAnim.TRANSISITION, {alpha: 1});        TweenLite.to(numbers, GlobalAnim.TRANSISITION, {alpha:0});        TweenLite.to(boxes, GlobalAnim.TRANSISITION, {alpha:0});        buttons.setState(buttons.getOffButtonState());    }    public function OVER():void {    }}}